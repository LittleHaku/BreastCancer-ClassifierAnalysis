@article{dindorf_classification_2021,
	title = {Classification and {Automated} {Interpretation} of {Spinal} {Posture} {Data} {Using} a {Pathology}-{Independent} {Classifier} and {Explainable} {Artificial} {Intelligence} ({XAI})},
