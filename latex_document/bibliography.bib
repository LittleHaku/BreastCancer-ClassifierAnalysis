	note = {Number: 1
Publisher: BioMed Central},
	keywords = {image},
	pages = {1--18},
	file = {Full Text PDF:/home/ivan/Zotero/storage/F2H8USH2/Sidey-Gibbons and Sidey-Gibbons - 2019 - Machine learning in medicine a practical introduc.pdf:application/pdf},
